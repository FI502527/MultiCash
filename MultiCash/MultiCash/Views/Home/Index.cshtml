@{
    ViewData["Title"] = "Home Page";
@model HomeViewModel

}

<div class="text-center">
    <div class="bank-group">
        <div class="bank-header">
            @Model.Bank.AccountType.ToUpper();
        </div>
        <div class="bank-info">
            <div class="bank-user">
                @Model.User.Email
            </div>
            <div class="bank-money">
                €@Model.Bank.Amount
            </div>
        </div>
    </div>
</div>
